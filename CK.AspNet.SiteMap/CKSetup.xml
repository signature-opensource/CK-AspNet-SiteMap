<Setup Engine="CK.Setup.StObjEngine, CK.StObj.Engine">
    <Aspect Type="CK.Setup.SetupableAspectConfiguration, CK.Setupable.Model" Version="1"/>
    <Aspect Type="CK.Setup.SqlSetupAspectConfiguration, CK.SqlServer.Setup.Model">
        <DefaultDatabaseConnectionString>
            Server=.;Database=CK_AspNet_SiteMap;Integrated Security=True;TrustServerCertificate=true
        </DefaultDatabaseConnectionString>
    </Aspect>

    <Aspect Type="CK.Setup.TypeScriptAspectConfiguration, CK.StObj.TypeScript">
    </Aspect>

    <BinPaths>
        <BinPath Path="../CK.AspNet.SiteMap/bin/[Debug|Release]/net6.0">
          <TypeScript TargetProjectPath="../Clients/ngx-ck-sitemap" AutoInstallYarn="True" GitIgnoreCKGenFolder="True">
          </TypeScript>
        </BinPath>
    </BinPaths>

    <WorkingDirectory>WorkingDir</WorkingDirectory>
    <LogLevel>Debug</LogLevel>
    <CKSetupName>CKSetup.AspNet.SiteMap</CKSetupName>
    <PreferredTargetRuntimes>Net60</PreferredTargetRuntimes>
    <ForceSetup>Full</ForceSetup>
</Setup>
